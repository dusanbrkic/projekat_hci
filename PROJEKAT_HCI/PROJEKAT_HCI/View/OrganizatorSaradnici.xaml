<Window x:Class="PROJEKAT_HCI.View.OrganizatorSaradnici"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PROJEKAT_HCI.View"
        mc:Ignorable="d"
        Title="OrganizatorSaradnici" Height="450" Width="800">
    <Grid Background="MediumPurple">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="105*"></ColumnDefinition>
            <ColumnDefinition Width="454*"></ColumnDefinition>
            <ColumnDefinition Width="191*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="37*"></RowDefinition>
            <RowDefinition Height="178*"></RowDefinition>
        </Grid.RowDefinitions>
        <Button  Click="Nazad_Btn_Click" Foreground="White" Background="Red" FontSize="17" Grid.Column="2">Nazad na glavnu stranu</Button>
        <Label Background="Purple" Grid.ColumnSpan="2" Foreground="White" FontSize="17" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">Saradnici</Label>
        <ScrollViewer  Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="3">
            <WrapPanel  Name="wrapper">
                
            </WrapPanel>
        </ScrollViewer>
    </Grid>
</Window>
